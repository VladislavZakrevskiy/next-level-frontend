	"main": "index.js",
	"scripts": {
		"start": "webpack serve --env port=3000 --env apiUrl=https://ulbi-course-server.vercel.app",
		"start:dev": "concurrently \"npm start\" \"npm run server:start\"",
		"start:vite": "vite",
		"start:dev:vite": "concurrently \"npm run start:vite\" \"npm run server:start\"",
		"build:prod": "webpack --env mode=production",
		"build:dev": "webpack --env mode=development",
		"test:unit": "jest --config ./config/jest/jest.config.ts",
		"test:ui": "jest --config ./config/jest/jest.config.ts --env=jsdom",
		"lint:ts": "eslint \"**/*.{ts,tsx}\"",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
		"lint:scss": "stylelint  \"**/*.{css,scss}\"",
		"lint:scss:fix": "stylelint  \"**/*.{css,scss}\" --fix",
		"storybook": "storybook dev -p 6006 -c ./config/.storybook",
		"build-storybook": "storybook build -c ./config/.storybook",
		"server:json": "json-server --watch ./json-server/db.json --port 8000",
		"server:start": "node ./json-server/index.js",
		"prepare": "husky install",
		"script:slice": "node ./scripts/createSlice/index.js",
		"script:updateImports": "ts-node ./scripts/updateImports/updateImports.ts",
		"script:shared-public-api": "ts-node ./scripts/createPublicApiForSharedUI/createPublicApiForSharedUI"
	},
